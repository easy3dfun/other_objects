mount_w = 100;
mount_h = 100;
mount_d = 5;

stick_distance = 30;

cube([mount_w, mount_h, mount_d], center=true);

translate([ stick_distance, 0, 0]) stick();
translate([-stick_distance, 0, 0]) stick();

module stick() {
    intersection() {
        rotate([ 60, 0, 0])
            cube([10,100,10], center=true);
        translate([0, 0, 20])
            cube([mount_w, mount_h, 40], center=true);
    }
}